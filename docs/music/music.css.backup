/* 2021-06-21 usp : Experimental */

svg.music text { 
	font-family : "Arial Narrow", sans-serif ;
	font-size : 12px ;
	text-anchor: middle ;
	}
svg.music g#index text {
	font-size : 8px ;
	}
svg.music circle {
	stroke-width : 0.8px ;
	stroke : black ;
	fill : white ;
	}
svg.music line {
	stroke-width : 0.2px ;
	stroke : black ;
	}
svg.music path {
	stroke-width : 0.2px ;
	stroke : black ;
	fill : white ;
	}
svg.music path.N {
	fill : #F0F0F0 ;
	}
svg.music path.N.root {
	fill : #ffd966;
	}